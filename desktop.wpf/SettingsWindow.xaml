<Window x:Class="FexSync.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FexSync"
        mc:Ignorable="d"
        Title="Settings" Height="350" Width="525" WindowStartupLocation="CenterScreen">
    <Grid>
        <TabControl Margin="0">
            <TabItem Header="Common Settings">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Account Settings">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
