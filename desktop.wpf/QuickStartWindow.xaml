<Window x:Class="FexSync.QuickStartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FexSync"
        mc:Ignorable="d"
        Title="Quick Start" Height="300" Width="300" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowState="Maximized">
    <Grid Name="QuickStartWindowGrid" Margin="0,0,0,0">

        <Button Name="ButtonBack" Content="Back" HorizontalAlignment="Right" Margin="0,0,174.667,15" VerticalAlignment="Bottom" Width="75" RenderTransformOrigin="0.742,-1.85" Command="{Binding CommandBack}" />

        <Button Name="ButtonNext" Content="Next" Margin="0,0,95,15" Width="75" VerticalAlignment="Bottom" HorizontalAlignment="Right" Command="{Binding CommandNext}"   />

        <Button Name="ButtonClose" Content="Close" Margin="0,0,15,15" Width="75" VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="ButtonClose_Click" />

        <Image Name="Image" Height="220" Width="295" Margin="0,0,0,50" Stretch="None" />

    </Grid>
</Window>
